{
  "manifest_version": 3,
  "name": "Autofillr",
  "description": "Automatically fill registration forms with consistently generated random data.",
  "version": "1.2.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "Autofillr-16.png",
      "32": "Autofillr-32.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "Autofillr-16.png",
    "32": "Autofillr-32.png",
    "192": "Autofillr-192.png",
    "512": "Autofillr-512.png"
  },
  "permissions": []
}
