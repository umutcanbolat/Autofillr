{
  "manifest_version": 2,
  "name": "Autofillr",
  "description": "Automatically fill forms with consistently generated data",
  "version": "0.1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Autofillr"
  },
  "content_scripts": [
    {
    "matches": [
        "http://*/*",
        "https://*/*"
        ],
    "js": ["content.js"],
    "all_frames": true
    }
], 
  "icons": {
    "16": "Autofillr-16.png",
    "32": "Autofillr-32.png",
    "192": "Autofillr-192.png",
    "512": "Autofillr-512.png"
  },
  "permissions": []
}
